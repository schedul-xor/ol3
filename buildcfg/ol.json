{

  "id": "ol",

  "css-allowed-unrecognized-properties": [
    "user-select"
  ],

  "css-inputs": [
    "../css/ol.css"
  ],

  "css-output-file": "../build/ol.css",

  "externs": [
    "../externs/bingmaps.js",
    "../externs/closure-compiler.js",
    "../externs/geojson.js",
    "../externs/oli.js",
    "../externs/olx.js",
    "../externs/proj4js.js",
    "../externs/tilejson.js",
    "../externs/topojson.js",
    "../externs/vbarray.js"
  ],

  "inherits": "base.json",

  "inputs": [
    "../build/exports.js"
  ],

  "output-wrapper": "// OpenLayers 3. see http://ol3js.org/\n(function(){%output%})();",

  "paths": [
    "../src"
  ]

}
